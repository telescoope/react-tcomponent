{
  "name": "react-tcomponent",
  "version": "1.1.8",
  "description": "Reusable react tcomponent for rapid react development",
  "author": "aldoetobex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/telescoope/react-tcomponent.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false --compress --format modern,cjs",
    "start": "microbundle-crl watch --css-modules false --compress --format modern,cjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@babel/plugin-transform-unicode-property-regex": "^7.22.5",
    "cross-env": "7.0.3",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "gh-pages": "3.2.3",
    "microbundle-crl": "0.13.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-scripts": "4.0.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@emotion/react": "11.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@wiris/mathtype-generic": "7.27.1",
    "apexcharts": "3.29.0",
    "axios": "0.24.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "5.1.3",
    "cleave.js": "1.6.0",
    "dropzone": "5.9.3",
    "html-react-parser": "1.4.0",
    "jodit-react": "1.1.10",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mousetrap": "1.6.5",
    "rc-time-picker": "3.7.3",
    "react-apexcharts": "1.3.9",
    "react-bootstrap": "2.0.1",
    "react-color": "2.19.3",
    "react-contenteditable": "3.3.6",
    "react-datepicker": "4.3.0",
    "react-doc-viewer": "^0.1.5",
    "react-dropzone-component": "3.2.0",
    "react-input-range": "1.3.0",
    "react-is-visible": "1.1.2",
    "react-loading-overlay": "1.0.1",
    "react-phone-number-input": "^3.1.37",
    "react-redux": "^7.2.4",
    "react-select": "^5.2.0",
    "react-spinners": "0.11.0",
    "react-table": "7.7.0",
    "react-tag-autocomplete": "6.3.0",
    "redux": "4.1.0",
    "styled-components": "5.3.3",
    "tcomponent": "git+https://github.com/telescoope/tcomponent.git",
    "video-react": "0.14.1"
  }
}
